        "jsCode": "// ============================================\n// SALES ANALYSIS & ANOMALY DETECTION MODULE\n// ============================================\n\n// Retrieve all input items from CSV extraction\nconst items = Object.values($input.all());\n\n// Fetch configurable threshold from Config node\nconst threshold = $('Config').first().json.alert_threshold;\n\n// --------------------------------------------\n// INPUT VALIDATION: Verify data presence\n// --------------------------------------------\nif (!items || items.length === 0) {\n  throw new Error('No data received from CSV');\n}\n\n// --------------------------------------------\n// DATA TRANSFORMATION: Parse and validate each row\n// --------------------------------------------\nconst cleanData = items.map((item, index) => {\n  const sales = parseFloat(item.json.Sales);\n  const date = new Date(item.json.Date);\n  \n  // Validate sales field is numeric\n  if (isNaN(sales)) {\n    throw new Error(`Row ${index + 1}: Invalid sales value`);\n  }\n  \n  // Validate date field is parseable\n  if (isNaN(date.getTime())) {\n    throw new Error(`Row ${index + 1}: Invalid date format`);\n  }\n  \n  return { sales, date };\n});\n\n// --------------------------------------------\n// EDGE CASE: Insufficient data for comparison\n// --------------------------------------------\nif (cleanData.length < 2) {\n  return {\n    current_sales: cleanData[0]?.sales.toFixed(2) || 0,\n    sales_change: 0,\n    status: 'INSUFFICIENT_DATA',\n    error: 'Need at least 2 weeks of data for comparison'\n  };\n}\n\n// --------------------------------------------\n// DATA PROCESSING: Sort chronologically and extract values\n// --------------------------------------------\ncleanData.sort((a, b) => a.date - b.date);\nconst currentSales = cleanData[cleanData.length - 1].sales;\nconst prevSales = cleanData[cleanData.length - 2].sales;\n\n// --------------------------------------------\n// EDGE CASE: Zero baseline prevents percentage calculation\n// --------------------------------------------\nif (prevSales === 0) {\n  return {\n    current_sales: currentSales.toFixed(2),\n    sales_change: 'N/A',\n    status: 'BASELINE_ZERO',\n    error: 'Previous week had $0 sales - cannot calculate percentage'\n  };\n}\n\n// --------------------------------------------\n// ANALYSIS: Calculate change and determine alert status\n// --------------------------------------------\nconst salesChange = ((currentSales - prevSales) / prevSales) * 100;\nconst status = salesChange < threshold ? 'VERIFY' : 'STABLE';\n\n// --------------------------------------------\n// OUTPUT: Return structured analysis object\n// --------------------------------------------\nreturn {\n  current_sales: currentSales.toFixed(2),\n  previous_sales: prevSales.to"
